   "source": [
    "F43 = GF(43)\n",
    "F43t.<t> = F43[]\n",
    "p = F43t(t^6+6)\n",
    "F43_6.<v> = GF(43^6, name='v', modulus=p)\n",
    "ExtBLS6 = EllipticCurve(F43_6, [0, 6])"
   ]
  },
